{
  "name": "grape-ui-react",
  "version": "0.8.6",
  "description": "A React component library built with styled-components.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=src/internals/webpack/webpack.config.js",
    "build": "babel src/ -d lib --ignore **/*.json && babel src/assets --out-dir lib/assets --copy-files",
    "build:clean": "rimraf ./build",
    "build:styleguide": "npx styleguidist build",
    "gemfury:publish": "npm publish --registry='https://npm.fury.io/napadev/'",
    "lint": "npm run lint:js",
    "lint:js": "npm run lint:eslint -- .",
    "lint:eslint": "eslint",
    "lint:fix": "eslint --fix .",
    "lint:watch": "esw -w src/**",
    "prepublish": "npm run build",
    "prerelease": "npm version prerelease --preid=\"$(git rev-parse --abbrev-ref HEAD)\" | sed -e \"s/\\//\\-/\"",
    "pretest": "npm run test:clean && npm run lint",
    "start": "npx styleguidist server",
    "test:clean": "rimraf ./coverage",
    "test:quick": "cross-env NODE_ENV=test jest",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:update": "cross-env NODE_ENV=test jest -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/napagroupnyc/grape-ui-react/grape-ui-react.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/napagroupnyc/grape-ui#readme",
  "jest": {
    "testURL": "http://localhost",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/*/RbGenerated*/*.{js,jsx}",
      "!src/index.js",
      "!src/global-styles.js",
      "!src/*/*/Loadable.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 91,
        "branches": 91,
        "functions": 91,
        "lines": 91
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/lib",
      "<rootDir>/src/internals",
      "<rootDir>/src/elements/typography/index.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/internals/testing/test-bundler.js"
    ],
    "testRegex": "tests/.*\\.test\\.js$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@styled-system/css": "5.1.4",
    "@styled-system/prop-types": "5.1.2",
    "@styled-system/theme-get": "5.1.2",
    "babel-polyfill": "6.26.0",
    "cleave.js": "1.5.3",
    "except": "0.1.3",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "polished": "3.4.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-checkbox-group": "4.0.1",
    "react-day-picker": "7.3.2",
    "react-dom": "16.12.0",
    "react-hook-form": "3.28.15",
    "react-hook-form-input": "1.1.10",
    "react-image": "2.2.0",
    "react-moment": "0.9.7",
    "react-router-dom": "5.0.0",
    "react-select": "2.4.4",
    "react-textarea-autosize": "7.1.0",
    "styled-components": "4.4.0",
    "styled-system": "5.1.4"
  },
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-es2015": "7.0.0-beta.53",
    "@babel/preset-react": "7.0.0",
    "acorn": "6.3.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-flow-react-proptypes": "25.1.0",
    "babel-plugin-react-intl": "4.1.19",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-plugin-webpack-alias": "2.1.2",
    "coveralls": "3.0.6",
    "cross-env": "6.0.0",
    "css-loader": "3.3.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.4.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-watch": "6.0.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "jest-styled-components": "6.3.3",
    "react-styleguidist": "10.3.2",
    "react-test-renderer": "16.12.0",
    "style-loader": "1.0.1",
    "webpack": "4.41.0"
  }
}
